<UserControl x:Class="Monarca.UI.WPF.Usuario.CustomControls.ItemTabCustom"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Monarca.UI.WPF.Usuario.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="{StaticResource PrimaryBrush}">
    <Button Style="{StaticResource ButtonItem}" Click="Button_Click">
        <TextBlock x:Name="txbTitulo" Style="{StaticResource WhiteTextTextBlockStyle}" VerticalAlignment="Center" FontWeight="Bold" FontSize="15"/>
    </Button>

</UserControl>
